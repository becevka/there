start = $time;

@ rand;

generate = {
    (?generated) : { $rand $options };
    item << $el;
    $num _ { item is generated };
    #opts << $options;
    #opts add cherry;
} ($num $options $env{rand});

check = {
    (?times) : {$what is? $type};
    '' + $what + ' is ' + $type + ': ' + times $print;
} ($what $type);

opts = [apple pear plum];

apple generate 100000 opts;

check apple apple;
check apple pear;
check apple plum;

end = $time;
end - start