tests = |describe test|;
> = {
    tests <<;
    tests + [$$el $$test];
} ($test);

should = {
        v << $el;
        $el ?! $expected _ {'' + v + ', but expected:' + $expected  $error};
} ($expected);

all = {
    tests <<;
    should <<;
    tests _ {
        test <<;
        test ($should) should;
    }
}